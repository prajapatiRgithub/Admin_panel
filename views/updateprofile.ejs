<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Update Profile Page</title>
    <!-- Latest compiled and minified CSS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">

    <!-- jQuery library -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>

    <!-- Popper JS -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>

    <!-- Latest compiled JavaScript -->
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.6.3/css/all.css"
        integrity="sha384-UHRtZLI+pbxtHCWp1t77Bi1L4ZtiqrqD80Kn4Z8NTSRyMA2Fd33n5dQ8lWUE00s/" crossorigin="anonymous">



    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.0/dist/css/bootstrap.min.css">
    <script src="https://code.jquery.com/ry-3jque.5.1.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.0/dist/js/bootstrap.bundle.min.js"></script>

    <style type="text/css">
        .node {
            border-style: solid;
        }
    </style>
</head>

<body>
    <div class="container-fluid">
            <div style="align-items: center;">

                <br>
                <h1 class="mt-8 p-2 bg-primary text-white" style="align-items: center; text-align: center;">Upadate
                    Profile</h1> <br>

                <% if (typeof error !=='undefined' && error.length> 0) { %>
          <div class="container">
            <div class="alert alert-warning" style="text-align: center;">
              <%= error %> &nbsp; &nbsp; &nbsp;<a href="/" style="color:#8a6d3b;">&#10006;</a>
            </div>
          </div>
          <% } %>
            <form action="/updateprofile" method="post" enctype="multipart/form-data">
              <div class="form-group">
                <label for="fname">First Name:</label>
                <input type="name" id="fname" class="form-control" name="fname" placeholder="Name" value="<%= values.fname %>">
                
                <% if (typeof error1 !=='undefined' && error1.length> 0) { %>
                  <div style="color: red;">
                    <%= error1 %>
                  </div>
                  <% } %>
              </div>
              <div class="form-group">
                <label for="mname">Middle Name:</label>
                <input type="name" id="Lname" class="form-control" name="mname" placeholder="Middle name">
      
                <% if (typeof error2 !=='undefined' && error2.length> 0) { %>
                  <div style="color: red;">
                    <%= error2 %>
                  </div>
                  <% } %>
              </div>
              <div class="form-group">
                <label for="lname">Last Name:</label>
                <input type="name" id="lname" class="form-control" name="lname" placeholder="last name">
      
                <% if (typeof error3 !=='undefined' && error3.length> 0) { %>
                  <div style="color: red;">
                    <%= error3%>
                  </div>
                  <% } %>
              </div> <br>
                    <div class="form-group">
                                
                                    <label for="gender">Gender:</label>
                                    <input type="radio" id="gender" name="gender" value="Male" <%=values.gender=='Male'
                                        ? 'checked' : '' %>> Male
                                    <input type="radio" id="gender" name="gender" value="Female"
                                        <%=values.gender=='Female' ? 'checked' : '' %>> Female
                                    <input type="radio" id="gender" name="gender" value="other"
                                        <%=values.gender=='other' ? 'checked' : '' %>> Other

                                    <% if (typeof error4 !=='undefined' && error4.length> 0) { %>
                                        <div style="color: red;">
                                            <%= error4 %>
                                        </div>
                                        <% } %>
                            </div><br>


                            <div class="form-group">
                                    <label for="hobby">Hobby:</label>
                                    <input type="checkbox" id="hobby" name="hobby" value="music"
                                        <%=values.hobby=='music' ? 'checked' : '' %> >Music
                                    <input type="checkbox" id="hobby" name="hobby" value="dance"
                                        <%=values.hobby=='dance' ? 'checked' : '' %> >Danceing
                                    <input type="checkbox" id="hobby" name="hobby" value="drive"
                                        <%=values.drive=='drive' ? 'checked' : '' %> >Driving
                                    <input type="checkbox" id="hobby" name="hobby" value="game" <%=values.hobby=='game'
                                        ? 'checked' : '' %> >Gaming

                                <% if (typeof error5 !=='undefined' && error5.length> 0) { %>
                                    <div style="color: red;">
                                        <%= error5 %>
                                    </div>
                                    <% } %>  
                            </div><br>
                            <div class="form-group">
                                <div class="col-md-9">
                                    <label for="mobile">Mobile.No.:</label>
                                    <input type="text" id="mobile" name="mobile" placeholder="Enter your mobile number"
                                        class="form-control" value="<%values.mobile%>">
                                </div>
                                <% if (typeof error6 !=='undefined' && error6.length> 0) { %>
                                    <div style="color: red;">
                                        <%= error6 %>
                                    </div>
                                    <% } %>
                            </div><br>
                            <div class="form-group">
                                
                                    <label for="Image" class="control-label">Upload Image:</label>
                                    <input class="form-control" style="height: 70px;
                                        padding: 20px;" class="form-control" type="file" name="Image"> 
                            </div><br>
                            <div class="form-group">
                                    <label for="city">City:</label>
                                    <select name="city">
                                        <option value=" ">Select City </option>
                                        <option value="Ahmedabad">Ahmedabad</option>
                                        <option value="Surat">Surat</option>
                                        <option value="Rajkot">Rajkot</option>
                                        <option value="Junagadh">Junagadh </option>

                                    </select>
                                <% if (typeof error8 !=='undefined' && error8.length> 0) { %>
                                    <div style="color: red;">
                                        <%= error8 %>
                                    </div>
                                    <% } %>
                            </div><br>

                            <div class="form-group">
                                
                                    <label for="Email">Email:</label>
                                    <input type="Email" id="Email" name="Email" placeholder="Enter your email" class="form-control"
                                        value="<%=values.Email%>">
                                    <% if (typeof error9!=='undefined' && error9.length> 0) { %>
                                        <div style="color: red;">
                                            <%= error9 %>
                                        </div>
                                       <% } %>
                            </div><br>
                            <div class="row">
                                <div class="col-md-9">
                                    <input class="btn btn-primary" style="margin: 0% 0% 5% 60%; align-items: center; "
                                        type="submit" value="Submit">
                                </div>
                                <div class="col-md-6"></div>
                            </div><br>
                        </form>
            </div>
        </div>
</body>
</html>